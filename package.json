{
  "name": "@mobileaction/ma-ui-kit",
  "version": "1.2.17",
  "description": "MobileAction UI Components with Vue.js",
  "license": "MIT",
  "main": ".eslintrc.js",
  "directories": {
    "core": "core"
  },
  "files": [
    "core"
  ],
  "scripts": {
    "serve": "NO_UPDATE_CHECK=1 serve storybook-static -s -c ../serve.json # this is relative to dist folder",
    "build": "yarn scss && node --max_old_space_size=7000 node_modules/rollup/dist/bin/rollup -c rollup.config.ts --environment BUILD:production",
    "clean": "rimraf css && rimraf core && rimraf directives && rimraf plugins",
    "test:unit": "vue-cli-service test:unit",
    "snap:update": "vue-cli-service test:unit -u",
    "test:cov": "vue-cli-service test:unit --coverage --runInBand --logHeapUsage",
    "lint:all": "eslint --ext .js,.ts,.mjs,.vue .",
    "lint:all-report": "mkdir -p reports/eslint && yarn lint:all --quiet --format junit --output-file reports/eslint/results.xml",
    "lint:fix": "eslint --fix .js,.ts,.mjs,.vue .",
    "loki:approve": "loki approve",
    "loki:update": "loki update",
    "loki:test": "loki test --chromeTolerance 1",
    "loki:build:test": "yarn storybook:build && loki --requireReference --reactUri file:./storybook-static",
    "build:watch": "rollup --watch -c --environment BUILD:production",
    "scss": "node-sass design-system/system.scss -o css/ && node-sass design-system/root.scss -o css/",
    "storybook:build": "yarn scss && build-storybook",
    "storybook:start": "yarn scss && start-storybook -s ./ -p 6006",
    "deploy-storybook": "storybook-to-ghpages -- --source-branch=main",
    "docs": "bash ./generateDocs.sh",
    "jest:clear": "jest --clearCache"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^5.14.0",
    "core-js": "3.21.1",
    "dayjs": "^1.8.28",
    "feather-icons": "^4.28.0",
    "inputmask": "5.0.6",
    "tslib": "^2.3.1",
    "vue": "2.6.14",
    "vue-quill-editor": "^3.0.6"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.16.4",
    "@babel/plugin-proposal-optional-chaining": "^7.16.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/vue-fontawesome": "^2.0.0",
    "@mobileaction/eslint-config": "^3.0.0",
    "@rollup/plugin-alias": "3.1.8",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@rollup/plugin-replace": "3.0.0",
    "@rollup/plugin-typescript": "8.3.0",
    "@storybook/addon-a11y": "^6.3.12",
    "@storybook/addon-actions": "6.3.12",
    "@storybook/addon-backgrounds": "6.3.12",
    "@storybook/addon-docs": "6.3.12",
    "@storybook/addon-knobs": "6.3.1",
    "@storybook/addon-links": "6.3.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storysource": "6.3.12",
    "@storybook/addon-viewport": "6.3.12",
    "@storybook/addons": "6.3.12",
    "@storybook/source-loader": "6.3.12",
    "@storybook/storybook-deployer": "^2.8.10",
    "@storybook/vue": "6.3.12",
    "@types/feather-icons": "4.7.0",
    "@types/jest": "27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-plugin-typescript": "^4.3.1",
    "@vue/cli-plugin-unit-jest": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/test-utils": "^1.3.0",
    "@vuedoc/md": "^3.2.0",
    "@vuedoc/parser": "^3.3.0",
    "ant-design-vue": "^1.7.8",
    "autoprefixer": "^9",
    "babel-plugin-transform-builtin-classes": "^0.6.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-vue": "^2.0.2",
    "cross-env": "^7.0.2",
    "eslint": "^8.10.0",
    "fork-ts-checker-webpack-plugin": "^6.4.0",
    "fsevents": "^2.3.2",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "jest-environment-jsdom-sixteen": "^2.0.0",
    "jest-transform-stub": "^2.0.0",
    "loki": "^0.28.1",
    "node-sass": "^6.0.1",
    "postcss": "^7",
    "rimraf": "3.0.2",
    "rollup": "2.60.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "3.0.0",
    "rollup-plugin-svg": "2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.6",
    "sass": "^1.43.4",
    "sass-loader": "^10",
    "serve": "^13.0.2",
    "storybook-design-token": "^1.4.0",
    "svg-inline-loader": "^0.8.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^4.6.2",
    "vue-jest": "^3.0.7",
    "vue-loader": "15.9.8",
    "vue-template-compiler": "2.6.14"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mobileaction/ma-ui-kit.git"
  },
  "author": "MobileAction",
  "bugs": {
    "url": "https://github.com/mobileaction/ma-ui-kit/issues"
  },
  "homepage": "https://github.com/mobileaction/ma-ui-kit#readme"
}
